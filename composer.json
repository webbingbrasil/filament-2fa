{
    "name": "webbingbrasil/filament-2fa",
    "description": "A 2FA plugin for filament.",
    "keywords": [
        "laravel",
        "filament",
        "2fa"
    ],
    "homepage": "https://github.com/webbingbrasil/filament-2fa",
    "license": "MIT",
    "authors": [
        {
            "name": "Danilo Andrade",
            "email": "danilo@webbingbrasil.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "bacon/bacon-qr-code": "^2.0",
        "filament/filament": "^3.2",
        "pragmarx/google2fa": "^7.0|^8.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "filament/upgrade": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Webbingbrasil\\FilamentTwoFactor\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webbingbrasil\\FilamentTwoFactor\\FilamentTwoFactorProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
